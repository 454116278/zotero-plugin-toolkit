{
  "name": "zotero-plugin-toolkit",
  "version": "0.0.3",
  "description": "Toolkit for Zotero plugins",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && api-extractor run --local --verbose && api-documenter markdown --input-folder ./temp --output-folder ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingwind/zotero-plugin-toolkit.git"
  },
  "keywords": [
    "zotero",
    "plugin",
    "developer",
    "toolkit"
  ],
  "author": "windingwind",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windingwind/zotero-plugin-toolkit/issues"
  },
  "homepage": "https://github.com/windingwind/zotero-plugin-toolkit#readme",
  "dependencies": {
    "zotero-types": "^0.1.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "docs/**/*"
  ],
  "directories": {
    "lib": "dist",
    "doc": "docs"
  }
}